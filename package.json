{
  "name": "registry-sync",
  "version": "3.3.3",
  "description": "synchronize a remote npm registry for private use",
  "repository": "https://github.com/heikkipora/registry-sync",
  "bin": {
    "registry-sync": "bin/sync"
  },
  "scripts": {
    "build": "./build-npm",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check --loglevel warn .",
    "eslint": "eslint --fix --format=codeframe --ext .ts .",
    "eslint:check": "eslint --max-warnings=0 --format=codeframe --ext .ts .",
    "lint-staged": "lint-staged --verbose",
    "test": "mocha -r ts-node/register --timeout 120000 test/*.ts",
    "release-test": "cd release-test && ./run-sync-install-cycle.sh"
  },
  "author": "Heikki Pora",
  "license": "MIT",
  "dependencies": {
    "@yarnpkg/lockfile": "1.1.0",
    "axios": "0.27.2",
    "commander": "9.3.0",
    "semver": "7.3.7",
    "ssri": "9.0.1",
    "tar-fs": "2.1.1"
  },
  "devDependencies": {
    "@arkweid/lefthook": "0.7.7",
    "@types/chai": "4.3.1",
    "@types/lodash": "4.14.182",
    "@types/mocha": "9.1.1",
    "@types/node": "16.11.38",
    "@types/semver": "7.3.9",
    "@types/ssri": "7.1.1",
    "@types/tar-fs": "2.0.1",
    "@types/yarnpkg__lockfile": "1.1.5",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "chai": "4.3.6",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-formatter-codeframe": "7.32.1",
    "eslint-plugin-mocha": "10.0.5",
    "express": "4.18.1",
    "lint-staged": "13.0.0",
    "mocha": "10.0.0",
    "prettier": "2.6.2",
    "ts-node": "10.8.1",
    "typescript": "4.7.3"
  },
  "keywords": [
    "registry",
    "synchronize",
    "mirror",
    "private",
    "offline"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
