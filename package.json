{
  "name": "registry-sync",
  "version": "4.0.3",
  "description": "synchronize a remote npm registry for private use",
  "repository": "https://github.com/heikkipora/registry-sync",
  "bin": {
    "registry-sync": "bin/sync"
  },
  "scripts": {
    "build": "./build-npm",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check --loglevel warn .",
    "eslint": "eslint --fix --format=codeframe --ext .ts .",
    "eslint:check": "eslint --max-warnings=0 --format=codeframe --ext .ts .",
    "lint-staged": "lint-staged --verbose",
    "test": "mocha -r ts-node/register --timeout 120000 test/*.ts",
    "release-test": "cd release-test && ./run-sync-install-cycle.sh"
  },
  "author": "Heikki Pora",
  "license": "MIT",
  "dependencies": {
    "@yarnpkg/lockfile": "1.1.0",
    "axios": "1.4.0",
    "commander": "11.0.0",
    "semver": "7.5.3",
    "ssri": "10.0.4",
    "tar-fs": "3.0.3"
  },
  "devDependencies": {
    "@arkweid/lefthook": "0.7.7",
    "@types/chai": "4.3.5",
    "@types/lodash": "4.14.195",
    "@types/mocha": "10.0.1",
    "@types/node": "20.3.2",
    "@types/semver": "7.5.0",
    "@types/ssri": "7.1.1",
    "@types/tar-fs": "2.0.1",
    "@types/yarnpkg__lockfile": "1.1.6",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "chai": "4.3.7",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-formatter-codeframe": "7.32.1",
    "eslint-plugin-mocha": "10.1.0",
    "express": "4.18.2",
    "lint-staged": "13.2.2",
    "mocha": "10.2.0",
    "prettier": "2.8.8",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "keywords": [
    "registry",
    "synchronize",
    "mirror",
    "private",
    "offline"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
