{
  "name": "registry-sync",
  "version": "8.2.1",
  "description": "synchronize a remote npm registry for private use",
  "repository": "https://github.com/heikkipora/registry-sync",
  "type": "module",
  "bin": {
    "registry-sync": "bin/sync"
  },
  "scripts": {
    "build": "./build-npm",
    "eslint": "eslint --max-warnings=0 src test release-test/server/src",
    "fix-eslint": "eslint --fix src test release-test/server/src",
    "test": "node --test --test-timeout 120000 --test-concurrency 1 test/*.ts",
    "typecheck": "tsc",
    "release-test": "cd release-test && ./run-sync-install-cycle.sh",
    "refresh-manifests": "cd test/manifests && ./refresh-manifests.sh"
  },
  "author": "Heikki Pora",
  "license": "MIT",
  "dependencies": {
    "@yarnpkg/lockfile": "1.1.0",
    "axios": "1.13.6",
    "commander": "14.0.3",
    "lru-cache": "11.2.6",
    "semver": "7.7.4",
    "ssri": "13.0.1",
    "tar-fs": "3.1.1"
  },
  "devDependencies": {
    "@types/lodash": "4.17.24",
    "@types/node": "22.18.0",
    "@types/semver": "7.7.1",
    "@types/ssri": "7.1.5",
    "@types/tar-fs": "2.0.4",
    "@types/yarnpkg__lockfile": "1.1.9",
    "eslint": "9.39.2",
    "express": "5.2.1",
    "globals": "17.3.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.1"
  },
  "keywords": [
    "registry",
    "synchronize",
    "mirror",
    "private",
    "offline"
  ],
  "engines": {
    "node": ">=22.18.0"
  }
}
